<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jd.bdp.order.dao.UserDaoIface">

	<insert id="insertUserSingle" useGeneratedKeys="true" keyColumn="user_id"
		keyProperty="userId">
		insert users(user_name,sale,depart) values
		(#{userName},#{sale},#{depart});
	</insert>
 
 	<resultMap type="User" id="fixUser">
 		<id column="user_id" property="userId"/>
 		<result column="user_name" property="userName"/>
 		<result column="sale" property="sale"/>
 		<result column="depart" property="depart"/>
 	</resultMap>
 	<select id="getUsers" resultMap="fixUser">
 		select * from users limit 1,3 
  	</select>
</mapper>